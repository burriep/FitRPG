<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uwm.cs.fitrpg">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.HomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.GameActivity" />
        <activity android:name=".view.FitnessActivity" />
        <activity android:name=".activity.FitnessActivityTracking" />
        <activity
            android:name=".CombatActivity"
            android:parentActivityName=".view.HomeScreen" />
        <activity
            android:name=".MapActivity"
            android:parentActivityName=".view.HomeScreen" />

        <service
            android:name=".service.LocationUpdatesService"
            android:enabled="true" />

        <activity android:name=".activity.FitnessActivityHistory" />
    </application>

</manifest>